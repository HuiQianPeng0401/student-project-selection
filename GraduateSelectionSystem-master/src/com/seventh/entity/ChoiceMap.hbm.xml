<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.seventh.entity.ChoiceMap" table="choicemap">
		<id name="id" column="id" type="int">
			<generator class="identity"> <!-- 主键id的生成方式为自增 -->
			</generator>
		</id>
		<property name="studentId" column="studentId" type="string"></property>
		<property name="studentName" column="studentName" type="string"></property>
		<property name="topicId" column="topicId" type="int"></property>
		<property name="topicTitle" column="topicTitle" type="string"></property>
		<property name="state" column="state" type="int"></property>
		<property name="createTime" column="createTime" type="timestamp"></property>
	</class>
</hibernate-mapping>