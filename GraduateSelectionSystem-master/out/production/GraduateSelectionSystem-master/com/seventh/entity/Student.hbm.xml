<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.seventh.entity.Student" table="student">
		<id name="id" column="id" type="string"></id>
		<property name="password" column="password" type="string"></property>
		<property name="name" column="name" type="string"></property>
		<property name="gender" column="gender" type="string"></property>
		<property name="birthday" column="birthday" type="date"></property>
		<property name="classId" column="classid" type="int"></property>
		<property name="email" column="email" type="string"></property>
		<property name="qq" column="qq" type="string"></property>
		<property name="phone" column="phone" type="string"></property>
		<property name="autoMatch"  column="autoMatch" type="int"></property>
		<property name="isMatched"  column="isMatched" type="int"></property>
		<property name="choiceNum" column="choiceNum" type="int"></property>
	</class>
</hibernate-mapping>