<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.seventh.entity.Config" table="config">
		<id name="id" column="id" type="int"></id>
		<property name="stuMaxChoiceNum" column="stuMaxChoiceNum" type="int"></property>
		<property name="scrollingNotice" column="scrollingNotice" type="string"></property>
		<property name="teaMinTopicNum" column="teaMinTopicNum" type="int"></property>
		<property name="teaGuidanceNum" column="teaGuidanceNum" type="int"></property>
		<property name="topicChoiceLimit" column="topicChoiceLimit" type="int"></property>
	</class>
</hibernate-mapping>